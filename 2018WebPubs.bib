publisher = {NATURE PUBLISHING GROUP},
title = {{Impact-induced shock and the formation of natural quasicrystals in the early solar system}},
type = {Article},
url = {http://www.nature.com/doifinder/10.1038/ncomms5040},
volume = {5},
year = {2014}
}
@article{ISI:000340992700001,
abstract = {The postpolymerization modification of block copolymers has seen a
growing interest in the past decade ranging from fundamental synthesis
and structure property relationships to potential applications. The
resulting side-chain-modified block copolymers can retain the properties
