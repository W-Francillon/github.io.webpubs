pages = {590--595},
publisher = {NATURE PUBLISHING GROUP},
title = {{Switchable self-protected attractions in DNA-functionalized colloids}},
type = {Article},
volume = {8},
year = {2009}
}
@article{ISI:000267927500010,
abstract = {A bimetallic ruthenium olefin metathesis initiator was synthesized and
used to polymerize functionalized norbornenes, affording polymers that
