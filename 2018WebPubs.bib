journal = {Chem. Mater.},
keywords = {atomic force microscopy,calcium oxalate monohydrate,chemical force microscopy,cholesterol crystallization,crystal growth,cystine,cystinuria,gallstones,gout,grazing incidence X-ray diffraction,hemozoin,kidney stone,malaria,nucleation,pathological crystallization,uric acid},
number = {1},
pages = {477--495},
title = {{The materials science of pathological crystals}},
