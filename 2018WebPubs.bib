author = {Leunissen, Mirjam E and Dreyfus, Remi and Cheong, Fook Chiong and Grier, David G and Sha, Roujie and Seeman, Nadrian C and Chaikin, Paul M},
doi = {10.1038/NMAT2471},
issn = {1476-1122},
journal = {Nat. Mater.},
