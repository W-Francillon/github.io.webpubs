@article{ISI:000338644400007,
abstract = {We show numerically that the response of simple amorphous solids
(elastic networks and particle packings) to a local force dipole is
characterized by a lengthscale l(c) that diverges as unjamming is
approached as l(c) similar to (z - 2d)(-1/2), where z {\textgreater}= 2d is the mean
