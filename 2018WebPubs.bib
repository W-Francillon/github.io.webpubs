journal = {Chem. Sci.},
number = {3},
pages = {1319--1329},
publisher = {ROYAL SOC CHEMISTRY},
title = {{Site-specific inter-strand cross-links of DNA duplexes}},
type = {Article},
volume = {4},
year = {2013}
}
@article{ISI:000323069200023,
abstract = {We report the results of highly sensitive transmission X-ray scattering
measurements performed at the Advanced Photon Source, Argonne National
Laboratory, on nearly fully dense high-purity amorphous-silicon (a-Si)
