doi = {10.1002/ijch.201600071},
issn = {1869-5868},
journal = {Isr. J. Chem.},
keywords = {crystal growth,nucleation,polymorphism},
number = {1-2},
pages = {82--92},
title = {{Perils of Polymorphism: Size Matters}},
