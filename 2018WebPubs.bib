pages = {19922--19927},
publisher = {NATL ACAD SCIENCES},
title = {{Biomimetic peptoid oligomers as dual-action antifreeze agents}},
type = {Article},
volume = {109},
year = {2012}
}
@article{ISI:000308862300032,
abstract = {We study the relationship between microscopic structure and viscosity in non-Brownian suspensions. We argue that the formation and opening of contacts between particles in flow effectively leads to a negative selection of the contacts carrying weak forces. We show that an analytically tractable model capturing this negative selection correctly reproduces scaling properties of flows near the jamming transition. In particular, we predict that i) the viscosity eta diverges with the coordination number z as eta similar to (z(c) - z)(-( 3+theta)/( 1+theta)), ii) the operator which governs flow displays a low-frequency mode that controls the divergence of viscosity, at a frequency omega(min) similar to (z(c) - z)((3+theta)/(2+2 theta)), and iii) the distribution of forces displays a scale f{\{}*{\}} that vanishes near jamming as f{\{}*{\}}/ {\textless} f {\textgreater} similar to(z(c) - z)(1/(1+theta)) where theta characterizes the distribution of contact forces P(f) similar to f(theta) at jamming, and where z(c) is the Maxwell threshold for rigidity.},
