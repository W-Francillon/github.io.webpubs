issn = {1520-6106},
journal = {J. Phys. Chem. B},
month = {jul},
number = {28},
pages = {7981--7992},
pmid = {24716833},
title = {{Equilibrium Phase Behavior and Maximally Random Jammed State of Truncated Tetrahedra}},
url = {http://dx.doi.org/10.1021/jp5010133 http://pubs.acs.org/doi/abs/10.1021/jp5010133},
volume = {118},
