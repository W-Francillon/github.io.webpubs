title = {{Effect of dimensionality on the percolation thresholds of various {\textless}math display="inline"{\textgreater} {\textless}mi{\textgreater}d{\textless}/mi{\textgreater} {\textless}/math{\textgreater} -dimensional lattices}},
type = {Article},
url = {http://link.aps.org/doi/10.1103/PhysRevE.87.032149},
volume = {87},
