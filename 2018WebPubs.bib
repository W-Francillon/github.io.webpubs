doi = {10.1073/pnas.1120215109},
issn = {0027-8424},
journal = {Proc. Natl. Acad. Sci. U. S. A.},
keywords = {granular flows; jamming; rheology; viscoelasticity},
month = {mar},
number = {13},
pages = {4798--4803},
publisher = {NATL ACAD SCIENCES},
title = {{A unified framework for non-Brownian suspension flows and soft amorphous solids}},
type = {Article},
volume = {109},
year = {2012}
}
