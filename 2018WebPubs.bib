pages = {051309},
publisher = {AMER PHYSICAL SOC},
title = {{Hyperuniformity, quasi-long-range correlations, and void-space constraints in maximally random jammed particle packings. II. Anisotropy in particle shape}},
type = {Article},
url = {http://link.aps.org/doi/10.1103/PhysRevE.83.051309},
volume = {83},
year = {2011}
}
