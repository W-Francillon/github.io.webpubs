keywords = {granular; self-organization; limit cycles; frictio},
month = {jan},
number = {1},
pages = {49--53},
publisher = {NATL ACAD SCIENCES},
title = {{Precisely cyclic sand: Self-organization of periodically sheared frictional grains}},
type = {Article},
volume = {112},
year = {2015}
}
